package com.cpa.exception;

public class CPException {

	int errCode;
	String errMsg;
	
	
	//constructors

	public CPException() {
		super();
	}

	
	public CPException(int errCode, String errMsg) {
		super();
		this.errCode = errCode;
		this.errMsg = errMsg;
	}
	
	


	//getters & setters
	public int getErrCode() {
		return errCode;
	}
	
	public void setErrCode(int errCode) {
		this.errCode = errCode;
	}
	public String getErrMsg() {
		return errMsg;
	}
	public void setErrMsg(String errMsg) {
		this.errMsg = errMsg;
	}
	
	
	
	
}
